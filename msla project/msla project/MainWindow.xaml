<Window x:Class="msla_project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:msla_project"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FFCECECE">
        <InkCanvas x:Name="mainFrame" HorizontalAlignment="Left" Height="346" Margin="90,10,0,0" VerticalAlignment="Top" Width="693"/>
        <Button x:Name="openButton" Content="open" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top" Width="75" Click="openButton_Click"/>
        <Button x:Name="saveButton" Content="save" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="saveButton_Click"/>
    </Grid>
</Window>
